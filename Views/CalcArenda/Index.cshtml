@{
	Layout = null;
}
<!-- Расчет арендной платы -->
<div id="CalcArenda" class="row">
	<!-- Заголовок и кнопки -->
	<div class="row">
		<div class="col-md-6">
			<h3>Расчет арендной платы</h3>
		</div>
		<div class="col-md-6 text-right button-group-transaction">
			<button type="button" class="btn btn-primary btn-sm" onclick="CalcArenda.Calc.AddCalculation();">Добавить расчет</button>
			<button type="button" class="btn btn-primary btn-sm" onclick="CalcArenda.Calc.Prolongation();">Пролонгация договора</button>
			<button type="button" class="btn btn-primary btn-sm" onclick="CalcArenda.Calc.EarlyDissolution();">Досрочное расторжение</button>
		</div>
	</div>

	<!-- Расчеты -->
	<div id="CalcGridPlace" class="row"></div>
	<script type="text/javascript">
		CalcArenda.Calc.CreateGrid();
	</script>

	<!-- План/факт -->
	<div id="PlanFactGridPlace" class="row"></div>
	<script type="text/javascript">
		CalcArenda.PlanFact.CreateGrid();
	</script>
</div>