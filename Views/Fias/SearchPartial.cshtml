@{
	Layout = null;
}

<script type="text/javascript">
	window.fiasSearch = window.fiasSearch ? window.fiasSearch : new FiasSearch();//test
	window.fiasSearch.advancedSearchPartialUrl = '@Url.Action("AdvancedSearchPartial", "Fias")';
	window.fiasSearch.fiasServiceUrl = '@Settings.Default.FiasServiceUrl';
</script>

<table style="width: 100%;border: 0;" class="text_field fiasSearch">
	<tr>
		<td style="width: 100%; border: 0;">
			<table>
				<tr>
					<td>Поиск&nbsp;улицы:</td>
					<td style="width: 100%; border: 0;"></td>
					<td><a id="aFiasAdvancedSearch" href="#">Расширенный&nbsp;поиск</a></td>
				</tr>
			</table>
		</td>
	</tr>
	<tr>
		<td style="width: 100%; border: 0;">
			<input id="tbSearchString" name="SearchString" style="width:99%;" type="text" value="" class="advancedSearchSelect"/>
		</td>
	</tr>
	<tr>
		<td>
			<table>
				<tr>
					<td>
						<label for="divFiasPath">Результат поиска улицы:</label>
					</td>
					<td>
						<div id="divFiasPath"></div>
						<input id="hiddenFiasSearchKladrCode" type="hidden"/>
					</td>
				</tr>
			</table>
		</td>
	</tr>
	<tr>
		<td>
			<table>
				<tr>
					<td>
						Поиск дома:
					</td>
					<td>
						<div id="divHouse">
							<input id="slSearchHouse" type="text" name="slSearchHouse"@* style="width: 50px"*@/>
							<input id="hiddenSearchHouseId" type="hidden"/>
						</div>
					</td>
				</tr>
			</table>
		</td>
	</tr>
	<tr>
		<td>
			<label for="lblFiasFullAddress">Полный адрес:</label>
			<label id="lblFiasFullAddress">wqdwqdwqdqw</label>
		</td>
	</tr>
</table>

<div id="divFiasSearchButtons" class="text_field">
	<a href='javascript:void(0)' onclick='fiasSearchOk()'>Ок</a>
	<a href='javascript:void(0)' onclick='fiasSearchCancel()'>Отмена</a>
</div>
