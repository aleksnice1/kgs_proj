@using Kadastr.WebApp.Code.Extensions
@model IEnumerable<BaseEntityItemModel>
<div id="ajaxgrid">
    @{
        var grid = Html.Grid(Model);
    }
    @grid.GetHtml(
        columns: grid.Columns(
        grid.Column("", format: @<text><a href="#" class="action">Удалить</a></text>),
            grid.Column("Name", "Наименование")
            ))
</div>
