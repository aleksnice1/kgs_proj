@using Kadastr.WebApp.Code.Extensions
@model IEnumerable<Kadastr.Domain.clsAttributeCategory>
<div id="ajaxgrid">
    <br />
    @Html.ValidationMessageFor(model => Model)
    @{
        var grid = Html.Grid(Model);
    }
    @grid.GetHtml(
        columns: grid.Columns(
            grid.Column("sName", "Категория"),
            grid.Column("", format: @<text>@Html.ActionLink("Удалить",
                                                            "DeleteCategory", new { id = item.GUIDCategory })</text>, style: "action")))
</div>
